{
    "microservices": [
        {
            "name": "strapi-template-bundle",
            "stack": "spring-boot",
            "healthCheckPath": "/actuator/health",
            "dbms": "none",
            "ingressPath": "/entando-template-api/api",
            "commands": {
                "run": "mvn -Dspring-boot.run.arguments=\"--server.port=8081\" spring-boot:run"
            },
            "roles": [
                "et-first-role",
                "et-second-role"
            ]
        }
    ],
    "microfrontends": [
        {
            "name": "et-strapi-template-widget",
            "customElement": "et-strapi-template-app",
            "stack": "react",
            "type": "app-builder",
            "group": "free",
            "nav": [
                {
                    "label": {
                        "en": "strapi content temp",
                        "it": "modello di contenuto strapi"
                    },
                    "target": "internal",
                    "url": "/strapi-bundle"
                }
            ],
            "slot": "content",
            "paths": [
                "/strapi-bundle"
            ],
            "apiClaims": [
                {
                    "name": "int-api",
                    "type": "internal",
                    "serviceName": "strapi-template-bundle"
                },
                {
                    "name": "ext-api",
                    "type": "external",
                    "serviceName": "strapi-config-mcr-service",
                    "bundle": "registry.hub.docker.com/vijayhyadav/strapi-general-configs"
                }
            ]
        }
    ],
    "global": {
        "nav": [
            {
                "label": {
                    "en": "Entando Developers",
                    "it": "Entando Developers"
                },
                "target": "external",
                "url": "https://developer.entando.com/"
            }
        ]
    },
    "svc": ["keycloak"],
    "name": "entando-strapi-widgets-oci",
    "version": "0.0.1",
    "description": "entando-strapi-widgets-oci description",
    "type": "bundle"
}